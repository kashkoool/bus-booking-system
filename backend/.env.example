# MongoDB Connection
MONGO_URI=mongodb://localhost:27017/your_database_name

# Encryption
# Generate a secure 32-byte (64 hex characters) key using: openssl rand -hex 32
CARD_ENCRYPTION_KEY=your_64_character_hex_key_here

# Hashing Salt
# Generate a secure salt using: openssl rand -hex 16
CARD_HASH_SALT=your_salt_here

# Server Port
PORT=5001

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Environment
NODE_ENV=development
